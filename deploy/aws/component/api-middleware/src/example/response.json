{
    "contentType": "application/json",
    "hasData": true,
    "policy": {
        "local": {
            "RedactPassword": {
                "effect": "Transform",
                "processor": "Yodata",
                "type": "DataPolicy",
                "value": "{ \"password\": {\"value\": \"[PASSWORD]\"} }"
            }
        }
    },
    "request": {
        "method": "GET"
    },
    "response": {
        "body": "{'password':'secret'}",
        "headers": {
            "Content-Type": [
                "application/json"
            ]
        },
        "isBase64Encoded": false
    },
    "scope": {
        "NoDogsAllowed": {
            "condition": {
                "object": {
                    "type": "Dog"
                }
            },
            "effect": "Deny"
        }
    },
    "stage": "response"
}