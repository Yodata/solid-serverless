{
    "request": {
        "method": "GET"
    },
    "response": {
        "headers": {
            "Content-Type": [
                "application/json"
            ]
        },
        "body": "{\"password\": \"secret\"}"
    },
    "scope": {},
    "policy": {
        "local": {
            "RedactPassword": {
                "type": "DataPolicy",
                "processor": "Yodata",
                "effect": "Transform",
                "condition": "{ \"password\": {\"value\": \"[PASSWORD]\"} }"
            }
        }
    }
}